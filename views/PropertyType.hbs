<div class="container-fluid py-4">
      <div class="row">
        <div class="col-12">
          <div class="card my-4">
            <div class="card-header p-0 position-relative mt-n4 mx-3 z-index-2">
                <div class="bg-gradient-primary shadow-primary border-radius-lg pt-4 pb-3">
                <h6 class="text-white text-capitalize ps-3">Authors table</h6>
                <a  href="/property/propertyType_add"  class="btn btn-primary btn-rounded float-lg-end" style="margin-top:30px;">
                Add Property Type
                </a>
                </div>
            </div>
            <div class="card-body px-0 pb-2">
              <div class="table-responsive p-0">
                <table class="table align-items-center mb-0">
                  <thead>
                    <tr>
                      <th class="text-uppercase text-secondary text-xxs font-weight-bolder opacity-7 ps-2">Serial No</th>
                      <th class="text-uppercase text-secondary text-xxs font-weight-bolder opacity-7 ps-2">Property type</th>
                      <th class="text-center text-uppercase text-secondary text-xxs font-weight-bolder opacity-7">Status</th>
                      <th class="text-secondary opacity-7"></th>
                      <th class="text-secondary opacity-7"></th>

                    </tr>
                  </thead>
                  <tbody>
                    
                    
                    {{#if result}}
                      {{#each result}}                     
                    <tr>
                      <td>
                        <h2 class="text-xs text-secondary mb-0"> {{this.serial_no}}</h2>
                      </td>
                      <td>
                        <p class="text-xs font-weight-bold mb-0"> {{this.property_type_name}}</p>
                      </td>
                      <td class="align-middle text-center text-sm">
                        <a href="#">
                          {{#if this.status}}
                            <span class="badge badge-sm bg-gradient-primary"> Online </span>
                          {{else}}
                           <span class="badge badge-sm bg-gradient-secondary"> Offline </span>
                          {{/if}}
                        </a>
                      </td>
                      <td class="align-middle">
                        <a href="javascript:;" class="text-secondary font-weight-bold text-xs" data-toggle="tooltip" data-original-title="Edit user">
                          Edit
                        </a>
                      </td>
                      <td class="align-middle">
                        <a href="javascript:;" class="text-secondary font-weight-bold text-xs" data-toggle="tooltip" data-original-title="Edit user">
                          Delete
                        </a>
                      </td>
                    </tr>
                     {{/each}}
                    {{/if}}
                    
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
      </div>

